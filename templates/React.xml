<templateSet group="React">
  <template name="rrender" value="ReactDOM.render(&#10;  $component$&#10;  , document.querySelector('#example')&#10;)" description="渲染组件" toReformat="false" toShortenFQNames="true">
    <variable name="component" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="iife" value="(function(window){&#10;  $statement$&#10;})(window)" description="自调用匿名函数" toReformat="false" toShortenFQNames="true">
    <variable name="statement" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="rcjc" value="class $TM_FILENAME_BASE$ extends $ReactName$Component {&#10;   render() {&#10;         return (&#10;     &lt;div&gt;&#10;                         $END$&#10;                      &lt;/div&gt;&#10;               );&#10; }&#10;}&#10;" description="Creates a React component class with ES6 module system" toReformat="true" toShortenFQNames="true">
    <variable name="TM_FILENAME_BASE" expression="capitalize(camelCase(fileNameWithoutExtension()))" defaultValue="&quot;&quot;" alwaysStopAt="true" />
    <variable name="ReactName" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
</templateSet>