<templateSet group="html/xml">
  <template name="scr1" value="&lt;script type=&quot;text/javascript&quot;&gt;&#10;  $statement$&#10;&lt;/script&gt;" shortcut="SPACE" description="生成内置&lt;script&gt;" toReformat="false" toShortenFQNames="true">
    <variable name="statement" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="CD" value="&lt;![CDATA[&#10;$SELECTION$&#10;]]&gt;" description="CDATA区" toReformat="false" toShortenFQNames="false">
    <context>
      <option name="COMPLETION" value="false" />
    </context>
  </template>
  <template name="scr2" value="&lt;script type=&quot;text/javascript&quot; src=&quot;$src$&quot;&gt;&lt;/script&gt;" description="生成script链接" toReformat="false" toShortenFQNames="true">
    <variable name="src" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
</templateSet>